<%= render 'list', books: @books %>

<div class="col-md-8 offset-md-1">
    <h2>Book detail</h2>
    <table class="table"> 
    <tr>
        <td>
            <%= image_tag @user.get_profile_image(100,100)%>
        <br> 
            <%= @user.name %>
        </td>
        <td>
            <%= link_to book.title, book_path(book.id) %>
        </td>
        <td>
            <%= book.body %>
        </td>
    </tr>
</div>