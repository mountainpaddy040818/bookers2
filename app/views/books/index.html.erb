<%= render 'list', books: @books %>

 <div class="col-md-8 offset-md-1">
     <h2>Books</h2>
     <table class="table">
     <% @books.each do |book| %>
     <tr>
         <td>
             <%= image_tag @user.get_profile_image(100,100) %>
         </td>
         <td>
             <%= link_to book.title, book_path(book.id) %>
         </td>
         <td>
             <%= book.body %>
         </td>
         <td>
             <%= link_to "/books/edit", class: "btn btn-sm btn-success" do %>Edit<% end %>
         </td>
         <td>
             <%= link_to book_path(book.id), class: "btn btn-sm btn-danger", method: :delete do %>Destroy<% end %>
         </td>

     </tr>
     <% end %>

     </table>
  </div>
</div>
</div>